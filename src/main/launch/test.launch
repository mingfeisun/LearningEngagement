<launch>
    <include file="$(find file_server)/launch/ros_sharp_communication.launch">
        <param name="port" value="9090"/>
    </include>
    <arg name="urdf_file" default="$(find xacro)/xacro.py '$(find pepper_description)/urdf/pepper1.0_generated_urdf/pepper_robot.xacro'" />
    <param name="robot/name" value="Pepper"/>	
    <param name="robot_description" command="$(arg urdf_file)" />
</launch>