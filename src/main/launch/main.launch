<launch>
    <arg name="gui" default="true" />
    <arg name="version" default="1.0"/>

    <include file="$(find file_server)/launch/ros_sharp_communication.launch">
        <param name="port" value="9090"/>
    </include>
    <include file="$(find pepper_description)/launch/pepper_publisher.launch">
        <arg name="version" value="$(arg version)"/>
    </include>

    <node name="unity_server" pkg="unity_server" type="unity_human_server.py"/>

    <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" ></node>
    <node name="rviz" pkg="rviz" type="rviz" args="-f torso -d $(find pepper_description)/config/urdf.rviz" />
</launch>